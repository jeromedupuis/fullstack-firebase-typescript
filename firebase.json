{
  "hosting": {
    "public": "functions/public",
    "rewrites": [
      {
        "source": "/api/**",
        "function": "server"
      },
      {
        "source": "**",
        "function": "next"
      }
    ]
  },
  "functions": {
    "source": "functions",
    "predeploy": "npm --prefix \"$RESOURCE_DIR\" run build:public"
  }
}
