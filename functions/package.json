{
  "name": "fullstack-functions",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "serve": "yarn build && firebase emulators:start --only functions,hosting",
    "shell": "yarn build && firebase functions:shell",
    "start": "yarn shell",
    "deploy": "firebase deploy --only functions,hosting",
    "logs": "firebase functions:log",
    "predeploy": "yarn copy:modules && yarn install",
    "copy:modules:next": "node ../lib/copy-dependencies.js ../src/next ./",
    "copy:modules:server": "node ../lib/copy-dependencies.js ../src/server ./",
    "copy:modules": "yarn copy:modules:next && yarn copy:modules:server"
  },
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@types/next": "^9.0.0",
    "@types/react": "^16.9.43",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.1",
    "next": "^9.4.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.21",
    "firebase-functions-test": "^0.2.0",
    "typescript": "^3.8.0"
  }
}
